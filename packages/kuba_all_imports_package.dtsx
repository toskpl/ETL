          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="&#xA;&#xA;DECLARE @StartDate DATETIME &#xA;DECLARE @EndDate DATETIME &#xA;DECLARE @DateCounter DATETIME &#xA;&#xA;SET @StartDate = '01/01/1900'&#xA;SET @EndDate = '01/01/2050'&#xA;SET @DateCounter= @StartDate&#xA;&#xA;WHILE @DateCounter &lt; @EndDate&#xA;&#x9;BEGIN&#xA;&#x9;PRINT @DateCounter;&#xA;&#x9;INSERT INTO dim_dates VALUES(&#xA;&#x9;    @DateCounter,&#xA;&#x9;&#x9;CONVERT(VARCHAR(4),FORMAT(@DateCounter, 'yyyy')) ,&#xA;&#x9;&#x9;CONVERT(VARCHAR(2),FORMAT(@DateCounter, 'MM')) ,&#xA;&#x9;&#x9;CONVERT(VARCHAR(2),FORMAT(@DateCounter, 'dd')) ,&#xA;&#x9;&#x9;CONCAT('Q',DATENAME(QUARTER, @DateCounter)),&#xA;&#x9;&#x9;CONCAT(CONVERT(VARCHAR(4),FORMAT(@DateCounter, 'yyyy')),'-Q',DATENAME(QUARTER, @DateCounter))&#xA;&#x9;)&#xA;&#x9;&#xA;&#x9;SET @DateCounter=DATEADD(d,1,@DateCounter)&#xA;&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
